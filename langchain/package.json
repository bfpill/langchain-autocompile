{
    "name": "langchain-autocompile-agent",
    "version": "0.1.0",
    "private": true,
    "main": "src/index.ts",
    "bin": "dist/index.cjs",
    "type": "module",
    "scripts": {
      "dev": "nodemon src/index.ts",
      "build": "rimraf dist && esbuild src/index.ts --bundle --platform=node --outfile=dist/index.cjs",
      "ts.check": "tsc --project tsconfig.json"
    },
    "eslintConfig": {
      "extends": [
        "react-app",
        "react-app/jest"
      ]
    },
    "dependencies": {
      "body-parser": "^1.20.2",
      "express": "^4.18.2",
      "langchain": "^0.0.70",
      "nanoid": "^4.0.2"
    },
    "devDependencies": {
      "@types/body-parser": "^1.19.2",
      "@types/express": "^4.17.17",
      "@types/node": "^18.15.11",
      "esbuild": "^0.17.18",
      "nodemon": "^2.0.22",
      "rimraf": "^5.0.0",
      "ts-node": "^10.9.1",
      "typescript": "^5.0.4"
    }
  }
  